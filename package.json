  "scripts": {
    "start": "BROWSER='Firefox Developer Edition' export SET NODE_OPTIONS=--openssl-legacy-provider && export PORT=3030 && react-scripts start",
    "_comment1": "BROWSER='Firefox Developer Edition' export PORT=3010 && react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts --openssl-legacy-provider build",
    "_comment2": "GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "test:dev": "env-cmd -f .env.test cross-env NODE_OPTIONS=--openssl-legacy-provider PORT=3030 react-scripts start",
    "eject": "react-scripts eject",
    "postinstall": "patch-package",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended"
    ],
    "plugins": [
      "react",
      "react-hooks",
      "unused-imports"
    ],
    "rules": {
      "no-unused-expressions": "off",
      "react-hooks/exhaustive-deps": "warn",
      "react-hooks/rules-of-hooks": "error",
      "unused-imports/no-unused-imports": "warn",
      "unused-imports/no-unused-vars": [
        "warn",
        {
          "vars": "all",
          "varsIgnorePattern": "^_",
          "args": "after-used",
          "argsIgnorePattern": "^_"
        }
      ]
    }
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "eslint-plugin-react": "^7.37.4",
